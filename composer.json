{
  "name": "thefoovault/sf-skeleton",
  "autoload": {
    "psr-4": {
      "Shared\\": "src/Shared",
      "SymfonyClient\\": "apps/SymfonyClient/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "extra": {
    "bin-dir": "apps/SymfonyClient/bin",
    "config-dir": "apps/SymfonyClient/config",
    "src-dir": "apps/SymfonyClient/src",
    "var-dir": "apps/SymfonyClient/var",
    "public-dir": "apps/SymfonyClient/public"
  },
  "require": {
    "symfony/flex": "^1.13",
    "symfony/framework-bundle": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/console": "^5.3",
    "symfony/runtime": "^5.3",
    "symfony/messenger": "^5.3",
    "lambdish/phunctional": "^2.1",
    "ramsey/uuid": "^4.2"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "dg/bypass-finals": "^1.3"
  }
}
